[credential]
	helper = store
[url "ssh://git@github.com/"]
	insteadOf = https://github.com/
[user]
	name = thorntonmc
	email = mt@mthornton.dev
	signingkey = ssh-rsa 
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[gpg]
	format = ssh
[commit]
	gpgsign = true
